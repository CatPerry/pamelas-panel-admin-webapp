<p id="notice"><%= notice %></p>
<h1>ADMINS PORTAL</h1>


<h1>Manage</h1>
<%= link_to 'Teachers', teachers_path %>
<%= link_to 'Students', students_path %>
<%= link_to 'Courses', courses_path %>
<%= link_to 'Cohorts', cohorts_path %>
<%= link_to 'Home', admins_path %>

<%#="Welcome #{@adminSession.username}"%>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.username %></td>
        <td><%= admin.password %></td>
        <td><%= link_to 'Show', admin %></td>
        <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
        <td><%= link_to 'Destroy', admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin', new_admin_path %>
